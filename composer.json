{
  "name": "illiababak/restaurant-php-mvc-backend",
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Core\\": "core/"
    }
  },
  "scripts": {
    "start": "php -S localhost:8000 -t public -d log_errors=1 -d error_log=php://stdout"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.88"
  },
  "require": {
    "mongodb/mongodb": "^2.1",
    "vlucas/phpdotenv": "^5.6",
    "mailgun/mailgun-php": "^4.3",
    "nyholm/psr7": "^1.8",
    "guzzlehttp/guzzle": "^7",
    "php-http/guzzle7-adapter": "^1.1"
  },
  "config": {
    "process-timeout": 0,
    "allow-plugins": {
      "php-http/discovery": true
    }
  }
}
